# SYSTEM: Mathematica_10
# TEST: Prepend_Tests
# OUTPUT: output/Results/Mathematica_10/Prepend_Tests.json
ok  Prepend[{a, b, c, d}, x]    ==>     {x, a, b, c, d}
    ---
    data:
        got: {x, a, b, c, d}
        expect: {x, a, b, c, d}
        expect_evaluated: {x, a, b, c, d}
    ...
# Prepend to an RefLink[Association,paclet:ref/Association]:
ok  Prepend[<| 1 -> a, 2 -> b |>, 3 -> d]    ==>     <| 3 -> d, 1 -> a, 2 -> b |>
    ---
    data:
        got: <|3 -> d, 1 -> a, 2 -> b|>
        expect: <| 3 -> d, 1 -> a, 2 -> b |>
        expect_evaluated: <|3 -> d, 1 -> a, 2 -> b|>
    ...
# Prepend several rules to an RefLink[Association,paclet:ref/Association]:
ok  Prepend[<| 1 -> a, 2 -> b |>, {3 -> d, 4 -> e}]    ==>     <| 4 -> e, 3 -> d, 1 -> a, 2 -> b |>
    ---
    data:
        got: <|4 -> e, 3 -> d, 1 -> a, 2 -> b|>
        expect: <| 4 -> e, 3 -> d, 1 -> a, 2 -> b |>
        expect_evaluated: <|4 -> e, 3 -> d, 1 -> a, 2 -> b|>
    ...
# Use a head other than RefLink[List,paclet:ref/List]:
ok  Prepend[f[a, b, c], x + y]    ==>     f[x + y, a, b, c]
    ---
    data:
        got: f[x + y, a, b, c]
        expect: f[x + y, a, b, c]
        expect_evaluated: f[x + y, a, b, c]
    ...
# Prepend a row to a matrix:
ok  MatrixForm[Prepend[{{a, b}, {c, d}}, {x, y}]]    ==>     x*y*a*b*c*d # skip
# Prepend to each row in a matrix:
ok  MatrixForm[(Prepend[#1, x] & ) /@ {{a, b}, {c, d}}]    ==>     x*a*b*x*c*d # skip
# Prepend a vector to a matrix:
ok  MatrixForm[MapThread[Prepend, {{{a, b}, {c, d}}, {x, y}}]]    ==>     x*a*b*y*c*d # skip
# Successively prepend to a list:
ok  NestList[Prepend[#1, x] & , {a}, 5]    ==>     {{a}, {x, a}, {x, x, a}, {x, x, x, a}, {x, x, x, x, a}, {x, x, x, x, x, a}}
    ---
    data:
        got: {{a}, {x, a}, {x, x, a}, {x, x, x, a}, {x, x, x, x, a}, {x, x, x, x, x, a}}
        expect: {{a}, {x, a}, {x, x, a}, {x, x, x, a}, {x, x, x, x, a}, {x, x, x, x, x, a}}
        expect_evaluated: {{a}, {x, a}, {x, x, a}, {x, x, x, a}, {x, x, x, x, a}, {x, x, x, x, x, a}}
    ...
ok  Prepend[{a, b, c}, {x, y}]    ==>     {{x, y}, a, b, c}
    ---
    data:
        got: {{x, y}, a, b, c}
        expect: {{x, y}, a, b, c}
        expect_evaluated: {{x, y}, a, b, c}
    ...
ok  Flatten[%]    ==>     {x, y, a, b, c} # skip
